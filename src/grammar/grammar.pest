Operation = _{ PInc | PDec | VInc | VDec | Output | Input }
	PInc = {">" }
	PDec = { "<" }
	VInc = { "+" }
	VDec = { "-" }
	Output = { "." }
	Input = { "," }
Repetition = { "[" ~ Expr ~ "]" }

Term = _{ Operation | Repetition }
Expr = { (WHITESPACE* ~ Term ~ WHITESPACE*)* }

RsBrainfxxk = { SOI ~ Expr ~ EOI }
WHITESPACE = _{ " " | "\t" | "\n" }
